#include <iostream>

using namespace std;

int main(int argc, char** argv) {
	
	setlocale(LC_ALL, "");
	
	int dados[5] = {15,25,35,45,55};
	
	cout << "Um int Ocupa " << sizeof(int) << "Bytes" <<endl;
	
	for (int i=0; i<5; i++)
		cout << "Dados " << i << " = " << dados[i] <<endl;
		cout << "EndereÃ§o do Vetor Dados = " << dados <<endl;
	for (int i=0; i<5; i++)
		cout << "Dados " << i << " = " << *(dados + i) << endl;
		
	return 0;
}
